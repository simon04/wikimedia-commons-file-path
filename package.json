{
  "name": "wikimedia-commons-file-path",
  "version": "1.2.0",
  "description": "Returns the image/thumbnail file path for Wikimedia Commons",
  "main": "build/wikimedia-commons-file-path.js",
  "module": "index.js",
  "scripts": {
    "build": "rollup -c",
    "test": "npm run build && npm run test:mocha && npm run test:style",
    "test:mocha": "mocha",
    "test:style": "prettier --no-config --single-quote --check $(npm run git-ls-files)",
    "fix:style": "prettier --no-config --single-quote --write $(npm run git-ls-files)",
    "git-ls-files": "git ls-files '*.js' '*.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simon04/wikimedia-commons-file-path.git"
  },
  "keywords": [
    "wikimedia",
    "commons",
    "filepath"
  ],
  "author": "Simon Legner <Simon.Legner@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simon04/wikimedia-commons-file-path/issues"
  },
  "homepage": "https://github.com/simon04/wikimedia-commons-file-path#readme",
  "devDependencies": {
    "buble": "^0.19.7",
    "mocha": "^5.2.0",
    "prettier": "^1.18.2",
    "rollup": "^0.65.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^5.0.2",
    "uglify-js": "^3.6.0"
  },
  "dependencies": {
    "blueimp-md5": "^2.10.0"
  }
}
